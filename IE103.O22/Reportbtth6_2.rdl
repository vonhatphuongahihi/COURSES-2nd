    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
    ROW_NUMBER() OVER (ORDER BY HD.MSHD) AS STT,
    HD.MSHD AS N'MÃ SỐ HỘI ĐỒNG',
    DT.TENDT AS N'TÊN ĐỀ TÀI',
    HD.PHONG AS N'PHÒNG',
    CONVERT(VARCHAR, HD.NGAYHD, 103) AS N'NGÀY HỘI ĐỒNG',
    GV.TENGV AS N'CHỦ TỊCH HỘI ĐỒNG',
    COALESCE(
        (SUM(GV_HDDT.DIEM) + SUM(GV_PBDT.DIEM) + SUM(GV_UVDT.DIEM)) / 
        NULLIF(COUNT(GV_HDDT.MSGV) + COUNT(GV_PBDT.MSGV) + COUNT(GV_UVDT.MSGV), 0), 
    0) AS N'ĐIỂM TB'
FROM  
    HOIDONG HD
    INNER JOIN HOIDONG_DT HD_DT ON HD.MSHD = HD_DT.MSHD
    INNER JOIN DETAI DT ON HD_DT.MSDT = DT.MSDT
    INNER JOIN GIAOVIEN GV ON HD.MSGV = GV.MSGV
    LEFT JOIN GV_HDDT ON DT.MSDT = GV_HDDT.MSDT
    LEFT JOIN GV_PBDT ON DT.MSDT = GV_PBDT.MSDT
    LEFT JOIN GV_UVDT ON DT.MSDT = GV_UVDT.MSDT
GROUP BY 
    HD.MSHD, DT.TENDT, HD.PHONG, HD.NGAYHD, GV.TENGV;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
