      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT D.MSDT, D.TENDT AS 'TEN DE TAI', H.TENGV AS 'TEN GVHD',  G.MSGV AS 'MS GVHD'
FROM DETAI D
JOIN GV_HDDT G ON D.MSDT = G.MSDT
