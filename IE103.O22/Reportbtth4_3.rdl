  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
    DT.MSDT,
    DT.TENDT,
    HD.MSHD,
    TinhDiemTrungBinhDeTai.DiemTrungBinh
FROM 
    DETAI DT
    INNER JOIN (
        SELECT 
            MSDT,
            dbo.TinhDiemTrungBinhDeTai(MSDT) AS DiemTrungBinh
        FROM 
            DETAI
    ) AS TinhDiemTrungBinhDeTai ON DT.MSDT = TinhDiemTrungBinhDeTai.MSDT
    LEFT JOIN HOIDONG_DT HD_DT ON DT.MSDT = HD_DT.MSDT
    LEFT JOIN HOIDONG HD ON HD_DT.MSHD = HD.MSHD;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
